git --version

git config --list

git init

git add -A === to add all file in that folder to git

git status

git reset

git log

git clone <url> <local address to store file>   use . for current directory

git remote -v                   show remote 

git branch -a

git diff

git pull origin master

git push origin master

git branch <branch name>

git branch    ==== check out branches

git checkout <branch name>    

git checkout master

git pull origin master --allow-unrelated-histories

git merge <branch name>  === change directory to master first

git push origin --delete <branch name> ====  delete branch on git

git branch -d <branch name>   === delete local branch
